{
    "manifest_version": 2,
    "author": "Amr Fathy",
    "name": "OpenAI rtl",
    "version": "1.1.0",
    "description": "Change direction for arabic content",
    "permissions": ["activeTab"],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_title": "OpenAI Arabic direction"
    },
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": ["index.js"],
        "css": ["style.css"]
      }
    ]
  }
  